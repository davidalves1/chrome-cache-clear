{
  "manifest_version": 2,

  "name": "Chrome Cache Clear",
  "description": "Extens√£o para realizar a limpeza do cache do navegador",
  "version": "1.0",
  "icons": {
    "44": "assets/trash_icon-44.png",
    "128": "assets/trash_icon-128.png",
    "512": "assets/trash_icon-512.png"

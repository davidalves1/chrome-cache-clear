{
  "manifest_version": 2,

  "name": "Chrome Cache Clear",
  "description": "Limpa o cache do navegador",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/trash_icon-128.png",
    "default_popup": "popup.html",
    "default_title": "Chrome Cache Clear"
  },
  "permissions": [
      "browsingData"
  ]
}
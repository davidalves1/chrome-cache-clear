  "author": "David Alves de Souza",
  "browser_action": {
    "default_icon": "assets/trash_icon-128.png",
    "default_popup": "popup.html",
    "default_title": "Chrome Cache Clear"
  },
  "permissions": [
      "browsingData"
  ]
}